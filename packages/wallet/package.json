    "@uniswap/router-sdk": "1.8.0",
    "@uniswap/sdk-core": "4.1.2",
    "@uniswap/universal-router-sdk": "1.7.1",
    "@uniswap/v2-sdk": "4.1.0",
    "@uniswap/v3-sdk": "3.10.2",
    "apollo-link-rest": "0.9.0",
    "axios": "1.6.5",
