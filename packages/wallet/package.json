{
  "name": "wallet",
  "version": "0.0.0",
  "scripts": {
    "prepare": "yarn tradingapi:generate && yarn i18n:generate",
    "check:deps:usage": "depcheck",
    "i18n:generate": "i18next-resources-for-ts interface -i ./src/i18n/locales/source -o ./src/i18n/locales/@types/resources.d.ts",
    "lint": "eslint . --ext ts,tsx --max-warnings=0 && eslint src/i18n/locales/source/en-US.json",
    "lint:fix": "eslint . --ext ts,tsx --fix && eslint src/i18n/locales/source/en-US.json --fix",
    "test": "jest",
    "tradingapi:schema": "curl https://api.uniswap.org/v2/trade/api.json -o ./src/data/tradingApi/api.json",
    "tradingapi:generate": "openapi --input ./src/data/tradingApi/api.json --output ./src/data/tradingApi/__generated__ --client axios --useOptions --exportServices true --exportModels true",
    "snapshots": "jest -u",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@apollo/client": "3.9.6",
    "@ethersproject/bignumber": "5.7.0",
