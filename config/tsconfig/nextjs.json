{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "NextJS",
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "es2019",
    "lib": ["dom", "dom.iterable", "esnext"],
    "paths": {
      "react-native": ["react-native-web"]
    },
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  }
}
