    "elliptic": "6.5.7",
    "eslint-plugin-ft-flow": "2.0.3",
    "eslint-plugin-import": "2.27.5",
    "expo-local-authentication@13.8.0": "patch:expo-local-authentication@npm%3A13.8.0#./.yarn/patches/expo-local-authentication-npm-13.8.0-3a1b5c983f.patch",
    "expo-screen-capture@5.8.1": "patch:expo-screen-capture@npm%3A5.8.1#./.yarn/patches/expo-screen-capture-npm-5.8.1-78e5827fdf.patch",
    "expo/uuid": "3.4.0",
    "immer": "9.0.21",
    "is-core-module": "2.13.0",
    "json5": "2.2.2",
