  "scripts": {
    "android": "react-native run-android",
    "apollo:generate": "graphql-codegen --config codegen.ts",
    "apollo:schema": "get-graphql-schema $(grep UNISWAP_API_BASE_URL .env | cut -d '=' -f2)/v1/graphql -h X-API-KEY=$(grep UNISWAP_API_KEY .env | cut -d '=' -f2) -h Origin=$(grep UNISWAP_APP_URL .env | cut -d '=' -f2) > ./src/data/__generated__/schema.graphql",
    "clean": "react-native-clean-project",
    "contracts:compile": "yarn contracts:compile:abi",
    "contracts:compile:abi": "typechain --target ethers-v5 --out-dir src/abis/types './src/abis/**/*.json'",
    "debug": "react-devtools",
    "deduplicate": "yarn-deduplicate --strategy=fewer",
    "deploy:ios:local:dev": "yarn prepare-for-pr && fastlane ios buildAndShip --env dev",
    "deploy:ios:local:beta": "yarn prepare-for-pr && fastlane ios buildAndShip --env beta",
    "deploy:ios:local:prod": "yarn prepare-for-pr && fastlane ios buildAndShip --env prod",
    "env:fastlane:download": "bash ./scripts/downloadFastlaneEnv.sh",
    "env:fastlane:upload": "bash ./scripts/uploadFastlaneEnv.sh",
    "env:local:download": "bash ./scripts/downloadEnvLocal.sh",
    "env:local:upload": "bash ./scripts/uploadEnvLocal.sh",
    "firestore:deploy:rules": "firebase deploy --only firestore:rules",
    "hardhat": "hardhat node",
    "i18n:extract": "i18next",
    "ios": "SKIP_BUNDLING=1 react-native run-ios",
    "ios:smol": "SKIP_BUNDLING=1 react-native run-ios --simulator=\"iPhone SE (3rd generation)\"",
    "ios:dev:release": "react-native run-ios --configuration Dev",
    "ios:beta": "react-native run-ios --configuration Beta",
    "ios:release": "react-native run-ios --configuration Release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && eslint src/locales/en/translation.json",
    "lmu": "yarn prepare-for-pr",
    "postinstall": "patch-package",
    "prepare-for-pr": "yarn && yarn pod && yarn-deduplicate && yarn lint --fix && yarn tsc:compile && yarn test -u && yarn apollo:generate && yarn i18n:extract",
    "start": "NODE_ENV=development react-native start",
    "start:production": "NODE_ENV=production react-native start --reset-cache",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:chromatic": "chromatic --build-script-name=storybook:build",
    "test": "jest",
    "test:e2e:build": "RN_SRC_EXT=e2e.js detox build -c ios.sim.debug",
    "test:e2e:packager": "RN_SRC_EXT=e2e.js,e2e.ts yarn start:production --no-interactive",
    "test:e2e:run": "RN_SRC_EXT=e2e.js detox test -c ios.sim.debug",
    "tsc:compile": "tsc --project .",
    "unicons": "cd scripts && python3 populate_svgs.py && cd .. && yarn lint --fix",
    "pod": "./scripts/podinstall.sh"
  },
