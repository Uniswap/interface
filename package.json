  "dependencies": {
    "@actions/core": "1.10.1",
    "@commitlint/cli": "17.5.0",
    "@commitlint/config-conventional": "17.4.4",
    "@crowdin/cli": "3.14.0",
    "@ethersproject/keccak256": "5.7.0",
    "@manypkg/cli": "0.19.2",
    "danger": "11.2.6",
    "depcheck": "1.4.7",
    "dotenv-cli": "7.1.0",
    "husky": "8.0.3",
    "i18next": "23.10.0",
