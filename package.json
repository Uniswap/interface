{
  "name": "@uniswap/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "contracts:compile": "yarn contracts:compile:abi && yarn contracts:compile:v3",
    "contracts:compile:abi": "typechain --target ethers-v5 --out-dir src/abis/types './src/abis/**/*.json'",
    "contracts:compile:v3": "typechain --target ethers-v5 --out-dir src/abis/uniswapV3/types './node_modules/@uniswap/?(v3-core|v3-periphery)/artifacts/contracts/**/*.json'",
    "debug": "react-devtools",
    "deploy:ios:alpha": "fastlane ios buildAndShip",
    "firestore:deploy:rules": "firebase deploy --only firestore:rules",
    "hardhat": "hardhat node",
    "i18n:extract": "i18next",
    "ios": "SKIP_BUNDLING=1 react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "start": "NODE_ENV=development react-native start",
    "start:production": "NODE_ENV=production react-native start --reset-cache",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:chromatic": "chromatic --build-script-name=storybook:build",
    "test": "jest",
    "test:e2e:build": "RN_SRC_EXT=e2e.js detox build -c ios.sim.debug",
    "test:e2e:packager": "RN_SRC_EXT=e2e.js,e2e.ts yarn start:production --no-interactive",
    "test:e2e:run": "RN_SRC_EXT=e2e.js detox test -c ios.sim.debug",
    "tsc:compile": "tsc --project .",
    "unicons": "cd scripts && python3 populate_svgs.py && cd .. && yarn lint --fix"
  },
