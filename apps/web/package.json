{
  "name": "@uniswap/interface",
  "version": "1.1.0",
  "description": "Uniswap Interface",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "prepare": "bun ajv",
    "ajv": "bun run scripts/compile-ajv-validators.js",
    "graphql:generate": "echo 'GraphQL generation handled by uniswap package'",
    "anvil:mainnet": "dotenv -- bash -c 'RUST_LOG=debug anvil --print-traces --fork-url https://${REACT_APP_QUICKNODE_ENDPOINT_NAME}.quiknode.pro/${REACT_APP_QUICKNODE_ENDPOINT_TOKEN} --hardfork prague --no-rate-limit'",
    "anvil:base": "dotenv -- bash -c 'RUST_LOG=debug anvil --print-traces --fork-url https://${REACT_APP_QUICKNODE_ENDPOINT_NAME}.base-mainnet.quiknode.pro/${REACT_APP_QUICKNODE_ENDPOINT_TOKEN} --hardfork prague --port 8546'",
    "check:deps:usage": "depcheck",
    "check:circular": "concurrently \"../../scripts/check-circular-imports.sh ./src/pages/App.tsx 2\" \"../../scripts/check-circular-imports.sh ./src/setupTests.ts 0\"",
    "sitemap:generate": "bun run scripts/generate-sitemap.js",
    "start": "ROLLDOWN_OPTIONS_VALIDATION=loose vite dev",
    "dev": "ROLLDOWN_OPTIONS_VALIDATION=loose vite dev",
    "build:production": "CLOUDFLARE_ENV=production ROLLDOWN_OPTIONS_VALIDATION=loose vite build",
    "build:staging": "CLOUDFLARE_ENV=staging ROLLDOWN_OPTIONS_VALIDATION=loose vite build --mode staging",
    "preview": "ROLLDOWN_OPTIONS_VALIDATION=loose vite preview",
    "analyze": "source-map-explorer 'build/assets/*.js' --no-border-checks --gzip",
    "format": "biome check . --linter-enabled=false",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location node_modules/.cache/eslint/ --max-warnings=0 .",
    "lint:fix": "eslint --ignore-path .gitignore --cache --cache-location node_modules/.cache/eslint/ . --fix",
    "playwright:test": "playwright test",
    "playwright:test:anvil": "playwright test --grep '\\.anvil\\.'",
    "playwright:test:no-anvil": "playwright test --grep-invert '\\.anvil\\.'",
    "typecheck": "tsc && bun run typecheck:cloud",
    "typecheck:cloud": "tsc -p functions/tsconfig.json",
    "find:unused": "bash scripts/delete-unused-assets.sh",
    "test": "NODE_OPTIONS='--no-deprecation' vitest run",
    "test:set1": "NODE_OPTIONS='--no-deprecation' vitest run src/components",
    "test:set2": "NODE_OPTIONS='--no-deprecation' vitest run src/pages src/state",
    "test:set3": "NODE_OPTIONS='--no-deprecation' vitest run src/hooks src/nft src/utils",
    "test:set4": "NODE_OPTIONS='--no-deprecation' vitest run src --exclude 'src/components/**' --exclude 'src/pages/**' --exclude 'src/state/**' --exclude 'src/hooks/**' --exclude 'src/nft/**' --exclude 'src/utils/**'",
    "test:watch": "NODE_OPTIONS='--no-deprecation' vitest watch",
    "test:bundle": "bun run -r esbuild-register ./src/test-utils/bundle-size-test.ts",
    "snapshots": "NODE_OPTIONS='--no-deprecation' vitest run -u",
    "test:cloud": "NODE_OPTIONS='--no-deprecation' vitest run functions --config functions/vitest.config.ts",
    "deduplicate": "echo 'Bun automatically deduplicates dependencies'",
    "storybook:run": "storybook dev -p 6006",
    "storybook:run:with-tests": "concurrently -s second -n \"SB RUN,SB TEST WATCH\" -c \"magenta,blue\" \"bun run storybook:run --quiet\" \"wait-on tcp:127.0.0.1:6006 -t 600000 && bun run storybook:test --watch\"",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook --excludeTags=\"no-tests\" --testTimeout 60000",
    "storybook:test:standalone": "concurrently -k -s first -n \"SB BUILD,SB TEST\" -c \"magenta,blue\" \"bun run storybook:build && http-server storybook-static --port 6006 --silent\" \"wait-on --timeout 600000 tcp:127.0.0.1:6006 && bun run storybook:test --maxWorkers=2\""
  },
  "lint-staged": {
    "bun.lock": [
      "echo 'Bun automatically deduplicates dependencies'"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
