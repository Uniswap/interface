{
  "name": "@uniswap/mobile",
  "version": "0.0.1",
  "private": true,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "android": "rnef run:android --variant=devDebug --app-id-suffix=dev && bun run start",
    "android:release": "rnef run:android --variant=devRelease --app-id-suffix=dev",
    "android:beta": "rnef run:android --variant=betaDebug --app-id-suffix=beta",
    "android:beta:release": "rnef run:android --variant=betaRelease --app-id-suffix=beta",
    "android:prod": "rnef run:android --variant=prodDebug",
    "android:prod:release": "rnef run:android --variant=prodRelease",
    "check:deps:usage": "./scripts/checkDepsUsage.sh",
    "check:bundlesize": "./scripts/checkBundleSize.sh",
    "clean": "react-native-clean-project",
    "debug:reactotron:install": "./scripts/installDebugger.sh",
    "deduplicate": "echo 'Bun automatically deduplicates dependencies'",
    "depcheck": "depcheck",
    "env:android:keystore:download": "bash ./scripts/downloadAndroidKeystore.sh",
    "env:fastlane:download": "bash ./scripts/downloadFastlaneEnv.sh",
    "env:fastlane:upload": "bash ./scripts/uploadFastlaneEnv.sh",
    "env:local:download": "bash ../../scripts/downloadEnvLocal.sh xmznnx7ozuojy5lnohcmt73aee ../../.env.defaults.local && bun run env:local:copy:swift",
    "env:local:upload": "bash ../../scripts/uploadEnvLocal.sh xmznnx7ozuojy5lnohcmt73aee ../../.env.defaults.local",
    "env:local:copy:swift": "python3 scripts/copy_env_vars_to_swift.py",
    "e2e:prepare": "bun run e2e:generate-ids && bun run e2e:build-js",
    "e2e:generate-ids": "mkdir -p .maestro/scripts/dist && ts-node .maestro/scripts/tooling/generateTestIds.ts > .maestro/scripts/dist/testIds.js",
    "e2e:build-js": "ts-node .maestro/scripts/tooling/buildPerformanceScripts.ts",
    "e2e": "bun run e2e:prepare && maestro test .maestro/flows",
    "e2e:onboarding": "bun run e2e:prepare && maestro test .maestro/flows/onboarding",
    "e2e:swap": "bun run e2e:prepare && maestro test .maestro/flows/swap",
    "e2e:clear-logs": "rm -rf $HOME/.maestro/tests",
    "e2e:local:submit-metrics": "cd .maestro/scripts/performance && ./submit-local.sh && cd ../../..",
    "e2e:local:extract-metrics": "cd .maestro/scripts/performance && ./extract-metrics.sh && cd ../../..",
    "e2e:local:process-metrics": "bun run e2e:local:extract-metrics && bun run e2e:local:submit-metrics",
    "e2e:interactive": "bun run e2e:prepare && ts-node .maestro/scripts/yarn/e2e-interactive.ts",
    "firestore:deploy:rules": "firebase deploy --only firestore:rules",
    "link:assets": "react-native-asset",
    "check:circular": "../../scripts/check-circular-imports.sh ./src/app/App.tsx 0",
    "ios": "rnef run:ios --scheme Uniswap --configuration Debug && bun run start",
    "ios:interactive": "ts-node scripts/ios-build-interactive/main.ts",
    "ios:smol": "rnef run:ios --device=\"iPhone SE (3rd generation)\"",
    "ios:dev:release": "rnef run:ios --configuration Dev",
    "ios:beta": "rnef run:ios --configuration Beta",
    "ios:bundle": "rnef bundle --entry-file='index.js' --dev false --bundle-output='./ios/main.jsbundle' --sourcemap-output ./ios/main.jsbundle.map --dev=false --platform='ios' --assets-dest='./ios'",
    "ios:release": "rnef run:ios --configuration Release",
    "format": "biome check . --linter-enabled=false",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "NODE_ENV=development rnef start --client-logs",
    "start:e2e": "NODE_ENV=development IS_E2E_TEST=true rnef start --client-logs",
    "start:production": "NODE_ENV=production rnef start --reset-cache",
    "test": "node --max-old-space-size=8912 ../../node_modules/.bin/jest",
    "snapshots": "jest -u",
    "typecheck": "tsc -b",
    "unicons": "cd scripts && python3 populate_svgs.py && cd .. && bun run lint --fix",
    "pod": "./scripts/podinstall.sh",
    "pod:update": "./scripts/podinstall.sh -u",
    "storybook:generate": "sb-rn-get-stories",
    "prestart": "bun run storybook:generate",
    "softreset": "bash ./scripts/resetDevEnv.sh",
    "hardreset": "bash ./scripts/resetDevEnv.sh --hard"
  },
